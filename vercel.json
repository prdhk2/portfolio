{
    "version": 2,
    "builds": [
        {
            "src": "public/**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*.js",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*.png",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*.jpg",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*.svg",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}